  (call $assert_eq_i32 (i32.const 5701) (call $params_id ) (i32.const 3))
  (call $assert_eq_i32 (i32.const 5801) (call $param_break ) (i32.const 3))
  (call $assert_eq_i32 (i32.const 5901) (call $params_break ) (i32.const 3))
